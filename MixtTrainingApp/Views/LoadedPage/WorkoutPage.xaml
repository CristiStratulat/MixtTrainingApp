<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MixtTrainingApp.Views.LoadedPage.WorkoutPage"
             Background="White">
    <ContentPage.Content>
        <Grid Margin="20,20,20,20">
            <Grid.RowDefinitions>
                
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
        
            <Image Grid.Row="0" x:Name="Imagine"
                  Aspect="AspectFit"/>
            <Button Grid.Row="2" Text="Session Done" Clicked="Button_Clicked"/>
            
        </Grid>
    </ContentPage.Content>
</ContentPage>